<!-- @format -->

<!-- Mobile Navbar -->
<div class="mobile-navbar">
  <div id="close-nav-btn"><i class="fas fa-times"></i></div>
  <div id="open-nav-btn"><i class="fas fa-bars"></i></div>
  <div class="logo">
    <a href="/"><img src="/img/logo.png" alt="logo-house" /></a>
  </div>
  <div class="search-form">
    <form class="search-it" action="/main-results" method="POST">
      <input
        type="hidden"
        name="offset"
        placeholder=""
        value="0"
        disabled
      />
      <input
        type="hidden"
        name="limit"
        placeholder=""
        value="50"
        disabled
      />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="price_min"
        placeholder="Min &#xf0d6;"
        min="50000"
        <%if(price_min != null){%>
        value=<%-price_min%> 
        <%}else{%> 
        value="" <%}%> />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="price_max"
        placeholder="Max &#xf0d6;"
        min="50000"
        <%if(price_max!=null){%>
        value=<%-price_max%> 
        <%}else{%>
        value="" <%}%> />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="beds_min"
        placeholder="Rooms &#xf236;"
        min="1"
        max="4"
        <%if(beds_min!=null){%>
        value=<%-beds_min%>
        <%}else{%> 
        value="" <%}%> />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="baths_min"
        placeholder="Baths &#xf2cd;"
        <%if(baths_min!=null){%>
        value=<%-baths_min%>
        <%}else{%> 
        value="" <%}%> />
      <input
        type="text"
        name="city"
        placeholder="City"
        id="autocomplete2"
        required
        pattern="[a-zA-Z, ]+"
        <%if(city!=null){%>
        value="<%-city%>" 
        <%}else{%> value="" <%}%> />
      <input
        type="text"
        name="state_code"
        placeholder="State"
        required
        pattern="[a-zA-Z]+"
        <%if(state_code!=null){%>
        value=<%-state_code%> 
        <%}else{%> 
        value="" <%}%> />

      <select name="property_type" required>
        <option value="" disabled selected>Property Type</option>
        <option value="single_family">Single Family</option>
        <option value="multi_family">Multi Family</option>
        <option value="town_house">Town House</option>
        <option value="condos">Condo</option>
        <option value="land">Land</option>
        <option value="mobile">Mobile</option>
      </select>
      <button class="submit-search" type="submit">
        <i class="fa fa-search" aria-hidden="true"></i>
      </button>
    </form>
  </div>
  <div class="login">
    <!-- <button>Login</button> -->
    Source:
    <a href="https://rapidapi.com/datascraper/api/us-real-estate/"
      >US Real Estate API</a
    >
  </div>
</div>

<!-- End Mobile nav -->

