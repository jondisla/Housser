<!-- @format -->

<!-- Mobile Navbar -->
<div class="mobile-navbar">
  <div id="close-nav-btn"><i class="fas fa-times"></i></div>
  <div id="open-nav-btn"><i class="fas fa-bars"></i></div>
  <div class="logo">
    <a href="/"><img src="/img/logo.png" alt="logo-house" /></a>
  </div>
  <div class="search-form">
    <form class="search-it" action="/main-results" method="POST">
      <input
        type="hidden"
        name="offset"
        id=""
        placeholder=""
        value="0"
        disabled
      />
      <input
        type="hidden"
        name="limit"
        id=""
        placeholder=""
        value="50"
        disabled
      />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="price_min"
        id=""
        placeholder="Min &#xf0d6;"
        value=<%-price_min%>
       min="150000" max="700000"/>
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="price_max"
        id=""
        placeholder="Max &#xf0d6;"
        min="150000"
        max="700000"
        value=<%-price_max%>
      />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="beds_min"
        id=""
        placeholder="Rooms &#xf236;"
        value=<%-beds_min%>
        min="1"
        max="4"
      />
      <input
        class="fas fa-money-bill-wave"
        type="number"
        name="baths_min"
        id=""
        placeholder="Baths &#xf2cd;"
        value=<%-baths_min%>
        min="1"
        max="3"
      />
      <input
        type="text"
        name="city"
        id=""
        placeholder="City"
        required
        value=<%-city%>
        pattern="[a-zA-Z]+"
      />
      <input
        type="text"
        name="state_code"
        id=""
        placeholder="State"
        required
        value=<%-state_code%>
        pattern="[a-zA-Z]+"
      />

      <select name="property_type" required >
        <option value=<%-property_type%> disabled selected><%-property_type.replace(/-/g, ' ')%></option>
        <option value="single_family">Single Family</option>
        <option value="multi_family">Multi Family</option>
        <option value="town_house">Town House</option>
        <option value="condos">Condo</option>
        <option value="land">Land</option>
        <option value="mobile">Mobile</option>
      </select>
      <button class="submit-search" type="submit">
        <i class="fa fa-search" aria-hidden="true"></i>
      </button>
    </form>
  </div>
  <div class="login">
    <!-- <button>Login</button> -->
    Source:
    <a href="https://rapidapi.com/datascraper/api/us-real-estate/"
      >US Real Estate API</a
    >
  </div>
</div>

<!-- End Mobile nav -->

